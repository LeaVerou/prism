{
	"plugins": [
		"plugins/markdown",
		"./gulpfile.js/docs.js"
	],
	"opts": {
		"destination": "./docs",
		"template": "./node_modules/minami",
		"access": [
			"public",
			"protected"
		]
	},
	"recurseDepth": 10,
	"source": {
		"includePattern": "\\.js$",
		"excludePattern": "\\.min\\.js$"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"includeDate": false
		}
	}
}
