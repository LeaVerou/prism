"""foobar"""
"""fo"o
#bar
baz"""
'''foobar'''
'''fo'o
#bar
baz'''

Python supports empty triple quoted strings
"""""" """foo"""

----------------------------------------------------

[
	["triple-quoted-string", "\"\"\"foobar\"\"\""],
	["triple-quoted-string", "\"\"\"fo\"o\r\n#bar\r\nbaz\"\"\""],
	["triple-quoted-string", "'''foobar'''"],
	["triple-quoted-string", "'''fo'o\r\n#bar\r\nbaz'''"],
	"\r\n\r\nPython supports empty triple quoted strings\r\n",
	["triple-quoted-string", "\"\"\"\"\"\""],
	["triple-quoted-string", "\"\"\"foo\"\"\""]
]

