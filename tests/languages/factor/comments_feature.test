[
	[ "conventionally-named-word", "a!"],
	[ "comment", [ "! word" ] ],
	[ "normal-word", "!a"],
	[ "comment", [ "! word" ] ],
	[ "comment", [ "! comment" ] ],

	[ "normal-word", "!" ], [ "normal-word", "bad" ],
	[ "comment", [ "! \tfine" ] ],

	[ "comment", [ "! \"also a comment\"" ] ],
	[ "comment", [ "! : ( -- ) ;" ] ],
	[ "comment", [ "! ! leading comment-like token" ] ],
	[ "comment", [ "! whitespace before" ] ],
	[ "normal-word", "words" ],
	[ "normal-word", "blah" ],
	[ "comment", [ "! comment after code on a line" ] ]
]

