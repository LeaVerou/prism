styled.button`
	color: blue;
	background: ${props => props.background || 'none'};
`
styled(Button)`
	background: red;
`
createGlobalStyle`
	html {
		color: blue;
	}
`
css`
	color: blue;
`
keyframes`
	0% {
		color: blue;
	}
	100% {
		color: red;
	}
`

